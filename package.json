{
	"name": "react-webpack-base",
	"version": "1.0.0",
	"description": "Basic boostrap of react with webpack 4 and babel 7",
	"main": "index.js",
	"scripts": {
		"webpack": "webpack",
		"webpack-dev-server": "webpack-dev-server",
		"debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
		"prod": "npm run webpack -- --env.mode production",
		"dev": "npm run webpack-dev-server -- --env.mode development --hot --open",
		"prod:debug": "npm run debug -- --env.mode production",
		"dev:debug": "npm run debug -- --env.mode development",
		"prod:analyze": "npm run prod -- --env.presets analyze",
		"prod:compress": "npm run prod -- --env.presets compress",
		"start": "npm run dev",
		"dash": "webpack-dashboard -- npm run dev",
		"test": "mocha --opts ./test/unit/mocha.opts --recursive $(find ./test/unit -name '*.spec.js')",
		"e2e": "cypress open"
	},
	"author": "anishgeorgehb@gmail.com",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.0.0",
		"@types/chai": "^4.1.7",
		"babel-loader": "^8.0.4",
		"chai": "^4.2.0",
		"clean-webpack-plugin": "^2.0.1",
		"compression-webpack-plugin": "^2.0.0",
		"css-loader": "^1.0.1",
		"cypress": "^3.2.0",
		"enzyme": "^3.7.0",
		"enzyme-adapter-react-16": "^1.6.0",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.2.0",
		"jsdom": "^13.0.0",
		"jsdom-global": "^3.0.2",
		"mini-css-extract-plugin": "^0.4.4",
		"mocha": "^5.2.0",
		"node-sass": "^4.11.0",
		"prop-types": "^15.6.2",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"webpack": "^4.24.0",
		"webpack-bundle-analyzer": "^3.1.0",
		"webpack-cli": "^3.1.2",
		"webpack-dashboard": "^2.0.0",
		"webpack-dev-server": "^3.3.1",
		"webpack-merge": "^4.1.4"
	},
	"dependencies": {
		"react": "^16.6.0",
		"react-dom": "^16.6.0"
	}
}
